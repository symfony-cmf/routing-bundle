<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="symfony_cmf_routing.migrator_route_class">Symfony\Cmf\Bundle\RoutingBundle\Migrator\Route</parameter>
    </parameters>

    <services>

        <service id="symfony_cmf_routing.migrator.route" class="%symfony_cmf_routing.migrator_route_class%">
            <argument type="service" id="doctrine_phpcr"/>
            <argument>%symfony_cmf_routing.route_basepath%</argument>
            <tag name="doctrine_phpcr.migrator" alias="route" />
        </service>

    </services>
</container>